<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssafy.gumichelin.model.mapper.StoreMapper">

    <select id="selectStoreByType" parameterType="string" resultType="Store">
        SELECT * FROM t_store
        WHERE storeType = #{storeType}
    </select>

</mapper>